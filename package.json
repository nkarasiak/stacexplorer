{
  "name": "stac-explorer",
  "version": "2.6.0",
  "description": "Modern STAC Catalog Explorer with enhanced development workflow",
  "main": "proxy.js",
  "type": "module",
  "scripts": {
    "start": "node proxy.js",
    "dev": "concurrently \"npm run proxy\" \"npm run watch\"",
    "proxy": "node proxy.js",
    "watch": "nodemon --watch js --watch css --ext js,css --exec \"echo 'Files changed - refresh browser'\"",
    "serve": "http-server -p 3000 -c-1 --cors"
  },
  "dependencies": {
    "@deck.gl/core": "^9.1.13",
    "@deck.gl/geo-layers": "^9.1.13",
    "@deck.gl/json": "^9.1.13",
    "@deck.gl/layers": "^9.1.13",
    "@deck.gl/mapbox": "^9.1.13",
    "@loaders.gl/core": "^4.3.4",
    "@loaders.gl/tiles": "^4.3.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-fetch": "^2.6.9"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "http-server": "^14.1.1",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "stac",
    "geospatial",
    "earth-observation",
    "catalog",
    "explorer"
  ],
  "author": "Your Name",
  "license": "MIT"
}
