{
  "name": "stac-explorer",
  "version": "2.12.5",
  "description": "Modern STAC Catalog Explorer with enhanced development workflow",
  "main": "proxy.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build && open dist/stats.html",
    "build:prod": "NODE_ENV=production vite build",
    "build:gh-pages": "NODE_ENV=production vite build",
    "preview": "vite preview",
    "start": "npm run preview",
    "validate": "node validate-build.js",
    "test:build": "npm run build && npm run validate",
    "optimize": "npm run build:prod && npm run validate",
    "legacy:dev": "concurrently \"npm run proxy\" \"npm run watch\"",
    "legacy:start": "node proxy.js",
    "proxy": "node proxy.js",
    "watch": "nodemon --watch js --watch css --ext js,css --exec \"echo 'Files changed - refresh browser'\"",
    "serve": "http-server -p 3000 -c-1 --cors",
    "serve:python": "python serve.py -p 8000",
    "serve:simple": "python -m http.server 8000",
    "lint": "eslint js/ --ext .js",
    "lint:fix": "eslint js/ --ext .js --fix",
    "format": "prettier --write \"js/**/*.js\" \"css/**/*.css\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"js/**/*.js\" \"css/**/*.css\" \"*.{js,json,md}\"",
    "quality": "npm run lint && npm run format:check",
    "quality:fix": "npm run lint:fix && npm run format"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "minimatch": "^10.0.3",
    "node-fetch": "^2.6.9"
  },
  "devDependencies": {
    "@eslint/create-config": "^1.10.0",
    "@eslint/js": "^9.32.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cssnano": "^7.1.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "http-server": "^14.1.1",
    "nodemon": "^3.0.2",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser": "^5.31.6",
    "vite": "^5.4.10",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.21.1"
  },
  "sideEffects": [
    "*.css",
    "./js/app.js"
  ],
  "keywords": [
    "stac",
    "geospatial",
    "earth-observation",
    "catalog",
    "explorer"
  ],
  "author": "Your Name",
  "license": "MIT"
}
